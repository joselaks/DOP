<UserControl xmlns:syncfusion="http://schemas.syncfusion.com/wpf"  x:Class="DataObra.Documentos.Controles.PresDetalle"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DataObra.Documentos.Controles"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <syncfusion:ToolBarAdv>
            <StackPanel Orientation="Horizontal">
                <RadioButton x:Name="ListadoRadioButton" Content="Listado" GroupName="ViewMode" IsChecked="True" Checked="RadioButton_Checked"/>
                <RadioButton x:Name="AnalisisRadioButton" Content="Análisis" GroupName="ViewMode" Checked="RadioButton_Checked"/>
            </StackPanel>
        </syncfusion:ToolBarAdv>

    </Grid>
</UserControl>
