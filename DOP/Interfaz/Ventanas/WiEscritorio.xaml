<Window
    x:Class="DataObra.Interfaz.Ventanas.WiEscritorio"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:DataObra.Interfaz.Ventanas"
    xmlns:sfSkinManager="clr-namespace:Syncfusion.SfSkinManager;assembly=Syncfusion.SfSkinManager.WPF"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
    Title="DataObra"
    Width="1200"
    Height="800"
    MinWidth="800"
    mc:Ignorable="d"
     sfSkinManager:SfSkinManager.VisualStyle="Office2016White">
    <Window.Resources>
        <Style TargetType="syncfusion:TileViewItem">
            <Setter Property="HeaderVisibility" Value="Visible"/>
        </Style>
    </Window.Resources>
    <Grid>
        <syncfusion:TileViewControl Name="tileViewControl">
            <syncfusion:TileViewItem Header="Item1" x:Name="tile1"/>
            <syncfusion:TileViewItem Header="Item2" x:Name="tile2"/>
            <syncfusion:TileViewItem Header="Item3" x:Name="tile3"/>
            <syncfusion:TileViewItem Header="Item4" x:Name="tile4"/>
        </syncfusion:TileViewControl>


    </Grid>
</Window>
